{
  "name": "project-my-homepage",
  "author": "akky-xxxx <114.s.aki@gmail.com>",
  "repository": "https://github.com/akky-xxxx/project-my-homepage.git",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:strapi-photos": "yarn workspace service-strapi-photos dev",
    "dev:client": "yarn workspace service-client dev",
    "fix": "run-p fix:* -c",
    "fix:js": "prettier \"./*.js\" --write",
    "fix:client": "yarn workspace service-client fix",
    "fix:pack": "fixpack",
    "lint": "run-p lint:* -c",
    "lint:js": "prettier \"./*.js\" --check",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "fixpack": "^4.0.0",
    "husky": "^7.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "version": "1.0.0",
  "license": "MIT"
}
