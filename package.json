{
  "name": "project-my-homepage",
  "author": "akky-xxxx <114.s.aki@gmail.com>",
  "repository": "https://github.com/akky-xxxx/project-my-homepage.git",
  "engines": {
    "node": "16.15.0",
    "npm": "⚠️️  【yarn】 を使ってください。(((ꎤ’ω’)و三 ꎤ’ω’)-o≡【npm】",
    "yarn": "1.22.18"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:service-client": "sh ./scripts/build_push-service_client.sh",
    "build:service-sg-server": "sh ./scripts/build_push-service_sg_server.sh",
    "check-all": "run-p check-all:*",
    "check-all:common-icons": "yarn workspace common-icons check-all",
    "check-all:common-types": "yarn workspace common-types check-all",
    "check-all:root": "run-s fix lint",
    "check-all:service-client": "yarn workspace service-client check-all",
    "check-all:service-sg-server": "yarn workspace service-sg-server check-all",
    "check-all:terraform": "yarn workspace terraform check-all",
    "check-code": "run-p check-code:*",
    "check-code:common-icons": "yarn workspace common-icons check-code",
    "check-code:common-types": "yarn workspace common-types check-code",
    "check-code:root": "run-s fix lint",
    "check-code:service-client": "yarn workspace service-client check-code",
    "check-code:service-sg-server": "yarn workspace service-sg-server check-code",
    "check-code:terraform": "yarn workspace terraform lint",
    "deploy": "run-s deploy:*",
    "deploy:service-client": "sh ./scripts/deploy.sh service-client",
    "deploy:service-sg-server": "sh ./scripts/deploy.sh service-sg-server",
    "dev": "run-p dev:*",
    "dev:service-client": "yarn workspace service-client dev",
    "dev:service-sg-server": "yarn workspace service-sg-server dev",
    "fix": "run-p fix:* -c",
    "fix:config": "prettier \"./*.{js,json}\" \"./**/*.{js,json}\" --write",
    "fix:pack": "fixpack",
    "lint": "run-p lint:* -c",
    "lint:editorconfig": "editorconfig-checker",
    "lint:js": "prettier \"./*.js\" --check",
    "lint:pack": "fixpack --dryRun",
    "prepare": "husky install",
    "storybook": "run-p storybook:*",
    "storybook:common-icons": "yarn workspace common-icons storybook",
    "storybook:service-client": "yarn workspace service-client storybook",
    "test": "run-p test:*",
    "test:service-client": "yarn workspace service-client test",
    "test:service-sg-server": "yarn workspace service-sg-server test",
    "test:terraform": "yarn workspace terraform test"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "editorconfig-checker": "^4.0.2",
    "fixpack": "^4.0.0",
    "husky": "^8.0.1",
    "known-css-properties": "^0.25.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "version": "1.0.0",
  "license": "MIT"
}
