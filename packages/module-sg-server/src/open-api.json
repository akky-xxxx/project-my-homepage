{
  "openapi": "3.0.0",
  "info": {
    "title": "BFF",
    "version": "1.0"
  },
  "servers": [
    {
      "url": "http://localhost:5000"
    }
  ],
  "paths": {
    "/api/home": {
      "get": {
        "summary": "Get Main Visual Paths",
        "tags": [],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/responses/HomeGet"
                },
                "examples": {}
              }
            }
          }
        },
        "operationId": "get-api-home"
      },
      "parameters": []
    },
    "/api/photo-gallery": {
      "get": {
        "summary": "Get all photos",
        "tags": [],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/responses/PhotoGalleryGet"
                }
              }
            }
          }
        },
        "operationId": "get-api-photo-gallery"
      }
    }
  },
  "components": {
    "schemas": {},
    "responses": {
      "HomeGet": {
        "description": "Example response",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "mainVisualPaths": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  },
                  "readOnly": true
                }
              },
              "required": [
                "mainVisualPaths"
              ]
            },
            "examples": {
              "example-1": {
                "value": {
                  "mainVisualPaths": [
                    "http://placehold.jp/3d4070/ffffff/2000x1000.png?text=image1",
                    "http://placehold.jp/3d4070/ffffff/2000x1000.png?text=image2",
                    "http://placehold.jp/3d4070/ffffff/2000x1000.png?text=image3",
                    "http://placehold.jp/3d4070/ffffff/2000x1000.png?text=image4",
                    "http://placehold.jp/3d4070/ffffff/2000x1000.png?text=image5",
                    "http://placehold.jp/3d4070/ffffff/2000x1000.png?text=image6"
                  ]
                }
              }
            }
          }
        }
      },
      "PhotoGalleryGet": {
        "description": "Example response",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "cards": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "imagePath": {
                        "type": "string"
                      },
                      "location": {
                        "type": "object",
                        "required": [
                          "locationId",
                          "locationName"
                        ],
                        "properties": {
                          "locationId": {
                            "type": "string"
                          },
                          "locationName": {
                            "type": "string"
                          }
                        }
                      },
                      "takenAt": {
                        "type": "object",
                        "required": [
                          "yearMonth",
                          "viewTakenAt"
                        ],
                        "properties": {
                          "yearMonth": {
                            "type": "string",
                            "pattern": "^\\d{4}-\\d{2}$"
                          },
                          "viewTakenAt": {
                            "type": "string",
                            "pattern": "^\\d{4}(?:\\/\\d{2}){2}$"
                          }
                        }
                      },
                      "tags": {
                        "type": "array",
                        "items": {
                          "type": "object",
                          "properties": {
                            "tagId": {
                              "type": "string"
                            },
                            "tagName": {
                              "type": "string"
                            }
                          },
                          "required": [
                            "tagId",
                            "tagName"
                          ]
                        }
                      }
                    },
                    "required": [
                      "imagePath",
                      "location",
                      "takenAt",
                      "tags"
                    ]
                  }
                }
              },
              "required": [
                "cards"
              ]
            },
            "examples": {
              "example-1": {
                "value": {
                  "cards": [
                    {
                      "imagePath": "http://placehold.jp/3d4070/ffffff/2000x1000.png?text=image1",
                      "location": {
                        "locationId": "locationId1",
                        "locationName": "locationName1"
                      },
                      "takenAt": {
                        "yearMonth": "2022-01",
                        "viewTakenAt": "2022/01/02"
                      },
                      "tags": [
                        {
                          "tagId": "tagId1",
                          "tagName": "tagName1"
                        },
                        {
                          "tagId": "tagId2",
                          "tagName": "tagName2"
                        }
                      ]
                    },
                    {
                      "imagePath": "http://placehold.jp/3d4070/ffffff/2000x1000.png?text=image2",
                      "location": {
                        "locationId": "locationId2",
                        "locationName": "locationName2"
                      },
                      "takenAt": {
                        "yearMonth": "2022-01",
                        "viewTakenAt": "2022/01/02"
                      },
                      "tags": [
                        {
                          "tagId": "tagId1",
                          "tagName": "tagName1"
                        },
                        {
                          "tagId": "tagId2",
                          "tagName": "tagName2"
                        }
                      ]
                    },
                    {
                      "imagePath": "http://placehold.jp/3d4070/ffffff/2000x1000.png?text=image3",
                      "location": {
                        "locationId": "locationId2",
                        "locationName": "locationName2"
                      },
                      "takenAt": {
                        "yearMonth": "2022-01",
                        "viewTakenAt": "2022/01/02"
                      },
                      "tags": []
                    }
                  ]
                }
              }
            }
          }
        }
      }
    },
    "examples": {}
  }
}
