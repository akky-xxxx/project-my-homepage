{
  "name": "service-client",
  "scripts": {
    "build": "next build",
    "build:storybook": "build-storybook",
    "check-all": "npm-run-all fix -p spell-check lint:css type-check -s test",
    "check-code": "run-p spell-check lint type-check",
    "compare-story": "run-s -c ss:capture reg-suit:save reg-suit:open",
    "dev": "nodemon",
    "fix": "run-p fix:* -c",
    "fix-ts-by-eslint": "eslint \"./src/**/*.{ts,tsx}\" --fix",
    "fix-ts-by-prettier": "prettier \"./src/**/*.{ts,tsx}\" --write",
    "fix:config": "prettier \"./*.{js,json}\" \"./**/*.js\" \"!**/.next\" --write",
    "fix:pack": "fixpack",
    "fix:ts": "run-s fix-ts-by-eslint fix-ts-by-prettier",
    "hygen:ac": "hygen new atoms-component",
    "hygen:mc": "hygen new molecules-component",
    "hygen:pc": "hygen new pages-component",
    "hygen:tc": "hygen new templates-component",
    "lint": "run-p lint:* -c",
    "lint-ts-by-eslint": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint-ts-by-prettier": "prettier \"./src/**/*.{ts,tsx}\" --check",
    "lint:configs": "prettier \"./*.{js,json}\" \"./**/*.js\" \"!**/.next\" --check",
    "lint:css": "stylelint './src/**/*.{ts,tsx}'",
    "lint:ts": "run-s lint-ts-by-eslint lint-ts-by-prettier",
    "mock": "run-p mock:*",
    "mock:strapi": "prism mock ../service-strapi/src/extensions/documentation/documentation/1.0.0/full_documentation.json -p 4010",
    "reg-suit:open": "open-cli \"./report.html\"",
    "reg-suit:save": "reg-cli \"./.screenshot/actual\" \"./.screenshot/expected\" \"./.screenshot/diff\" -R \"./report.html\"",
    "spell-check": "cspell \"./src/**/*.{ts,tsx}\"",
    "ss:capture": "storycap http://localhost:3050 -o \".screenshot/actual\"",
    "ss:copy": "shx cp -r \"./.screenshot/actual/*\" \"./.screenshot/expected\"",
    "ss:git-add": "git add \"./.screenshot/expected\"",
    "ss:git-commit": "git commit -m \"docs(service-client): update expected screenshot@`date \"+%Y-%m-%d %H:%M:%S\"`\"",
    "ss:remove": "shx rm -rf \"./.screenshot/expected/*\"",
    "start": "next start",
    "storybook": "start-storybook -p 3050",
    "test": "run-s test:*",
    "test:client": "jest --coverage",
    "test:server": "jest -c jest.server.config.js",
    "type-check": "tsc",
    "update-ss": "run-s ss:remove ss:copy ss:git-add ss:git-commit"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "common-icons": "0.1.0",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "helmet": "^5.0.2",
    "next": "12.1.0",
    "qs": "^6.10.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-responsive-carousel": "^3.2.23",
    "remeda": "^0.0.32",
    "styled-components": "^5.3.3",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@stoplight/prism-cli": "^4.8.0",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-viewport": "^6.4.19",
    "@storybook/jest": "^0.0.9",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@storybook/testing-react": "^1.2.3",
    "@swc/core": "^1.2.148",
    "@swc/jest": "^0.2.20",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.39",
    "@types/styled-components": "^5.1.24",
    "@types/supertest": "^2.0.12",
    "@types/testing-library__jest-dom": "^5.14.3",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "cspell": "^5.18.5",
    "eslint": "^8.10.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-unicorn": "^41.0.0",
    "fixpack": "^4.0.0",
    "hygen": "^6.2.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "open-cli": "^7.0.1",
    "postcss-jsx": "^0.36.4",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.5.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "reg-keygen-git-hash-plugin": "^0.11.1",
    "reg-suit": "^0.11.1",
    "shx": "^0.3.4",
    "storybook-addon-next-router": "^3.1.1",
    "storybook-addon-pseudo-states": "^1.7.0",
    "storycap": "^3.1.5",
    "stylelint": "^14.5.3",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-no-unsupported-browser-features": "^5.0.3",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "supertest": "^6.2.2",
    "ts-node": "^10.6.0",
    "ts-unused-exports": "^8.0.0",
    "typescript": "^4.5.5"
  },
  "private": true,
  "version": "0.1.0"
}
