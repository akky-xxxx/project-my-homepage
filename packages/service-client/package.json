{
  "name": "service-client",
  "scripts": {
    "build": "next build",
    "build:storybook": "build-storybook",
    "check-all": "npm-run-all fix -p spell-check lint:css type-check -s test",
    "check-code": "run-p spell-check lint type-check",
    "compare-story": "run-s -c ss:capture reg-suit:save reg-suit:open",
    "dev": "next dev",
    "fix": "run-p fix:* -c",
    "fix-ts-by-eslint": "eslint \"./src/**/*.{ts,tsx}\" --fix",
    "fix-ts-by-prettier": "prettier \"./src/**/*.{ts,tsx}\" --write",
    "fix:config": "prettier \"./*.{js,json}\" \"./**/*.js\" \"!**/.next\" --write",
    "fix:pack": "fixpack",
    "fix:ts": "run-s fix-ts-by-eslint fix-ts-by-prettier",
    "hygen:ac": "hygen new atoms-component",
    "hygen:mc": "hygen new molecules-component",
    "hygen:pc": "hygen new pages-component",
    "hygen:tc": "hygen new templates-component",
    "lint": "run-p lint:* -c",
    "lint-ts-by-eslint": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint-ts-by-prettier": "prettier \"./src/**/*.{ts,tsx}\" --check",
    "lint:configs": "prettier \"./*.{js,json}\" \"./**/*.js\" \"!**/.next\" --check",
    "lint:css": "stylelint './src/**/*.{ts,tsx}'",
    "lint:editorconfig": "editorconfig-checker",
    "lint:pack": "fixpack --dryRun",
    "lint:ts": "run-s lint-ts-by-eslint lint-ts-by-prettier",
    "mock": "run-p mock:*",
    "mock:strapi": "prism mock ../service-strapi/src/extensions/documentation/documentation/1.0.0/full_documentation.json -p 4010",
    "reg-suit:open": "open-cli \"./report.html\"",
    "reg-suit:save": "reg-cli \"./.screenshot/actual\" \"./.screenshot/expected\" \"./.screenshot/diff\" -R \"./report.html\"",
    "spell-check": "cspell \"./src/**/*.{ts,tsx}\"",
    "ss:capture": "storycap http://localhost:3050 -o \".screenshot/actual\"",
    "ss:copy": "shx cp -r \"./.screenshot/actual/*\" \"./.screenshot/expected\"",
    "ss:git-add": "git add \"./.screenshot/expected\"",
    "ss:git-commit": "git commit -m \"docs(service-client): update expected screenshot@`date \"+%Y-%m-%d %H:%M:%S\"`\"",
    "ss:remove": "shx rm -rf \"./.screenshot/expected/*\"",
    "start": "next build && next start",
    "storybook": "start-storybook -p 3050",
    "test": "jest --coverage",
    "ts-unused-exports": "ts-unused-exports tsconfig.json",
    "type-check": "tsc",
    "update-ss": "run-s ss:remove ss:copy ss:git-add ss:git-commit"
  },
  "dependencies": {},
  "devDependencies": {},
  "private": true,
  "version": "0.1.0"
}
