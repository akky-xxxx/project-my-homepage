{
  "name": "common-icons",
  "main": "out/index.js",
  "scripts": {
    "build": "run-s build:compile build:staging build:commit",
    "build:commit": "git commit -m \"feat(common-icons): output components@`date \"+%Y-%m-%d %H:%M:%S\"`\"",
    "build:compile": "tsc --noEmit false",
    "build:staging": "git add ./out",
    "check-all": "run-s fix check-code",
    "check-code": "run-p lint type-check",
    "compare-story": "run-s -c ss:capture reg-suit:save reg-suit:open",
    "fix": "run-p fix:*",
    "fix:js": "prettier \"./*.{js,ts,tsx}\" \"./**/*.{js,ts,tsx}\" --write",
    "fix:pack": "fixpack",
    "gen": "svgr -d ./dist ./origin",
    "hygen:story": "hygen new story",
    "lint": "run-p lint:*",
    "lint:js": "prettier \"./*.{js,ts,tsx}\" \"./**/*.{js,ts,tsx}\" --check",
    "lint:pack": "fixpack --dryRun",
    "reg-suit:open": "open-cli \"./report.html\"",
    "reg-suit:save": "reg-cli \"./.screenshot/actual\" \"./.screenshot/expected\" \"./.screenshot/diff\" -R \"./report.html\"",
    "ss:capture": "storycap http://localhost:3051 -o \".screenshot/actual\"",
    "ss:copy": "shx cp -r \"./.screenshot/actual/*\" \"./.screenshot/expected\"",
    "ss:git-add": "git add \"./.screenshot/expected\"",
    "ss:git-commit": "git commit -m \"docs(common-icons): update expected screenshot@`date \"+%Y-%m-%d %H:%M:%S\"`\"",
    "ss:remove": "shx rm -rf \"./.screenshot/expected/*\"",
    "storybook": "start-storybook -p 3051",
    "type-check": "tsc",
    "update-ss": "run-s ss:remove ss:copy ss:git-add ss:git-commit"
  },
  "dependencies": {
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@storybook/react": "^6.4.21",
    "@svgr/cli": "^6.2.1",
    "fixpack": "^4.0.0",
    "hygen": "^6.2.0",
    "npm-run-all": "^4.1.5",
    "open-cli": "^7.0.1",
    "prettier": "^2.6.2",
    "reg-keygen-git-hash-plugin": "^0.11.1",
    "reg-suit": "^0.11.1",
    "shx": "^0.3.4",
    "storycap": "^3.1.7"
  },
  "private": true,
  "version": "0.1.0",
  "license": "MIT"
}
