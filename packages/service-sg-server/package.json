{
  "name": "service-sg-server",
  "main": "index.js",
  "scripts": {
    "build": "run-s build:compile build:staging build:commit",
    "build:commit": "git commit -m \"feat(service-sg-server): output bffApiClient@`date \"+%Y-%m-%d %H:%M:%S\"`\"",
    "build:compile": "tsc ./src/libs/bffApiClient/*.ts --noEmit false --outDir out/bffApiClient --declaration",
    "build:staging": "git add ./out",
    "check-all": "npm-run-all fix -p spell-check type-check -s test",
    "check-code": "run-p spell-check lint type-check",
    "dev": "nodemon",
    "fix": "run-p fix:* -c",
    "fix-ts-by-eslint": "eslint \"./src/**/*.ts\" --fix",
    "fix-ts-by-prettier": "prettier \"./src/**/*.ts\" --write",
    "fix:config": "prettier \"./*.{js,json}\" \"./**/*.js\" \"!**/.next\" --write",
    "fix:pack": "fixpack",
    "fix:ts": "run-s fix-ts-by-eslint fix-ts-by-prettier",
    "gen": "run-p gen:*",
    "gen:ts-api-client": "sh configs/shells/ts-api-client-gen.sh",
    "lint": "run-p lint:* -c",
    "lint-ts-by-eslint": "eslint \"./src/**/*.ts\"",
    "lint-ts-by-prettier": "prettier \"./src/**/*.ts\" --check",
    "lint:configs": "prettier \"./*.{js,json}\" \"./**/*.js\" \"!**/.next\" --check",
    "lint:editorconfig": "editorconfig-checker",
    "lint:pack": "fixpack --dryRun",
    "lint:ts": "run-s lint-ts-by-eslint lint-ts-by-prettier",
    "spell-check": "cspell \"./src/**/*.ts\"",
    "start": "NODE_ENV=production nodemon",
    "test": "jest --coverage",
    "ts-unused-exports": "ts-unused-exports tsconfig.json",
    "type-check": "tsc"
  },
  "dependencies": {},
  "devDependencies": {},
  "private": true,
  "version": "0.1.0",
  "license": "MIT"
}
